def categorize_cases(x):
    if x >= 10000:
        return 'High'
    elif x <= 200:
        return 'Low'
    else:
        return 'Medium'





cv_data['Category'] = cv_data['newCasesByPublishDate'].apply(categorize_cases)
cv_data.head(10)





cv_data['newCategory'] = cv_data['newCasesByPublishDate'].apply(lambda x: 'Red' if x >= 20000 else 'Amber')
